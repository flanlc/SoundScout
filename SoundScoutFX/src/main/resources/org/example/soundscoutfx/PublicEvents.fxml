<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="550.0" prefWidth="523.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.soundscoutfx.PublicEventsController">
   <children>
      <ListView fx:id="eventsView" layoutX="14.0" layoutY="74.0" onMouseClicked="#SetIndex" prefHeight="340.0" prefWidth="495.0" />
      <TextField fx:id="searchField" layoutX="113.0" layoutY="433.0" onKeyReleased="#HandleSearch" prefHeight="25.0" prefWidth="300.0" promptText="Enter Artist Name" />
      <ImageView fitHeight="50.0" fitWidth="200.0" layoutX="153.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
      <Button layoutX="225.0" layoutY="504.0" mnemonicParsing="false" text="Dashboard" />
      <Button layoutX="236.0" layoutY="468.0" mnemonicParsing="false" onAction="#DisplayEventDescription" text="Details" />
   </children>
</AnchorPane>
