<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="anchorField" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.example.soundscoutfx.EditProfileController">
   <children>
      <TextField fx:id="genreField" layoutX="228.0" layoutY="124.0" promptText="Genre" />
      <Text layoutX="271.0" layoutY="44.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Edit Profile" />
      <Button layoutX="258.0" layoutY="200.0" mnemonicParsing="false" onAction="#handlePhoto" text="Profile Picture" />
      <Button layoutX="574.0" layoutY="2.0" mnemonicParsing="false" onAction="#returnToDashboard" text="X" />
      <TextField fx:id="videoField" layoutX="229.0" layoutY="161.0" promptText="Performance URL" />
      <Button layoutX="278.0" layoutY="246.0" mnemonicParsing="false" onAction="#handleSubmit" text="Submit" />
   </children>
</AnchorPane>
